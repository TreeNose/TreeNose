{
    "grammar": {
        "call":{ 
            "python": [
                "call"
            ],
            "java": [
                "method_invocation"
            ],
            "javascript": [
                "call_expression"
            ]
        },
        "if_statement": {
            "python": [
                "if_statement"
            ],
            "java": [
                "if_statement"
            ],
            "javascript": [
                "if_statement"
            ]
        },
        "if_clause": {
            "python": [
                "if","elif"
            ],
            "java": [
                "if"
            ],
            "javascript": [
                "if"
            ]
        },
        "switch_statement": {
            "python": [
                "match_statement"
        ],
            "java": [
                "switch_expression"
            ],
            "javascript": [
                "switch_statement"
            ]
        },
        "switch_case": {
            "python": [
                "case_clause"
            ],
            "java": [
                "switch_block_statement_group"
            ],
            "javascript": [
                "switch_case", "switch_default"
            ]
        },
        "call_attribute": {
            "python": [
                "attribute"
            ],
            "java": [
                "field_access"
            ],
            "javascript": [
                "member_expression"
            ]
        },
        "walk_call_chains": {
            "python": [
                "attribute", "call"
            ],
            "java": [
                "field_access", "method_invocation"
            ],
            "javascript": [
                "member_expression", "call_expression"
            ]
        },
        "check_call_chains": {
            "python": [
                "attribute"
            ],
            "java": [
                "field_access", "method_invocation"
            ],
            "javascript": [
                "member_expression"
            ]
        },
        "attribute_access":{
            "python": [
                "attribute"
            ],
            "java": [
                "field_access"
            ],
            "javascript": [
                "member_expression"
            ]
        },
        "class_definition": {
            "python": [
                "class_definition"
            ],
            "java": [
                "class_declaration"
            ],
            "javascript": [
                "class_declaration"
            ]
        },
        "method_definition": {
            "python": [
                "function_definition"
            ],
            "javascript": [
                "method_definition",
                "function_declaration",
                "arrow_function",
                "generator_function_declaration",
                "function"
            ],
            "java": [
                "method_declaration", "constructor_declaration"
            ]
        },
        "parameters": {
            "python": [
                "parameters"
            ],
            "java": [
                "formal_parameters"
            ],
            "javascript": [
                "formal_parameters"
            ]
        },
        "field":{
            "python": [
                "attribute"
            ],
            "java": [
                "field_declaration"
            ],
            "javascript": [
                "field_definition"
            ]
        },
        "parameter": {
            "python": [
                "identifier",
                "default_parameter",
                "typed_parameter",
                "typed_default_parameter"
            ],
            "java": [
                "identifier"
            ],
            "javascript": [
                "identifier",
                "assignment_pattern",
                "array_pattern",
                "object_pattern"
            ]
        }
    },
    "node":{
        "parameters": {
            "java": [
                "formal_parameters"
            ],
            "python": [
                "parameters"
            ],
            "javascript": [
                "formal_parameters", "parameters"
            ]
        }
